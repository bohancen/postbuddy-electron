{
  "name": "electron-quick-start",
  "version": "1.0.2",
  "description": "A minimal Electron application",
  "main": "main.js",
  "build": {
    "productName": "postBuddy",
    "appId": "postBuddy0.0.1",
    "copyright": "xxxx",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "./icon.ico"
    }
  },
  "scripts": {
    "package": "electron-packager ./app postBuddy --platform=win32 --arch=x64 --icon=icon.ico --out=./out --asar --app-version=0.0.1",
    "package2": "electron-packager ./ postBuddy --platform=win32 --electron-version 6.1.5 --arch=x64 --icon=icon.ico --out=./out --overwrite --app-version=0.0.1",
    "package-win": "electron-builder build --win --x64",
    "start": "electron ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-multiparty": "^2.2.0",
    "express": "^4.17.1",
    "md5": "^2.2.1",
    "request": "^2.88.0"
  }
}
